<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆角控件属性-->
    <declare-styleable name="RadiusView">
        <!--圆角属性-->
        <attr name="radius_all" format="dimension" />
        <attr name="radius_leftTop" format="dimension" />
        <attr name="radius_rightTop" format="dimension" />
        <attr name="radius_rightBottom" format="dimension" />
        <attr name="radius_leftBottom" format="dimension" />

        <!--边框属性-->
        <attr name="solid_width" format="dimension" />
        <attr name="solid_color" format="color" />
        <!--背景颜色，系统的background将会失效-->
        <attr name="background_color" format="color" />
    </declare-styleable>
</resources>